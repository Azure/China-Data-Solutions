{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "{{Location}}"
        },
        "virtualMachineName": {
           "value": "{{Name}}"
        },
        "virtualMachineSize": {
            "value": "Standard_DS11"
        },
        "adminUsername": {
            "value": "{{VMUsername}}"
        },
        "virtualNetworkName": {
           "value": "{{Name}}-vnet"
        },
        "networkInterfaceName": {
           "value": "{{Name}}Netinterface"
        },
        "networkSecurityGroupName": {
            "value": "{{Name}}-nsg"
        },
        "adminPassword": {
            "value": "{{Password}}"
        },
        "diagnosticsStorageAccountName": {
            "value": "{{Name}}dstor"
        },
        "diagnosticsStorageAccountType": {
            "value": "Standard_LRS"
        },
        "diagnosticsStorageAccountId": {
            "value": "Microsoft.Storage/storageAccounts/{{Name}}dstor"
        },
        "addressPrefix": {
            "value": "10.2.0.0/24"
        },
        "subnetName": {
            "value": "default"
        },
        "subnetPrefix": {
            "value": "10.2.0.0/24"
        },
        "publicIpAddressName": {
             "value": "{{Name}}-ip"
        },
        "publicIpAddressType": {
             "value": "Static"
        }
    }
}