  </connectionStrings>
  <appSettings>
    <add key="EventHubName" value="{{EventHubName}}" />
    <add key="EventHubConnection" value="{{EventHubConnection}}" />
    <add key="EventHubConsumerGroup" value="{{EventHubConsumerGroup}}" />
    <add key="EventHubPartitions" value="{{EventHubPartition}}" />
    <add key="IoTHubHostName" value="{{IoTHubName}}" />
    <add key="IoTHubConnectionString" value="{{IoTHubConnection}}" />
    <add key="StorageURI" value ="{{StorageURI}}/upload/{0}" />
    <add key="StorageConnectString" value="{{StorageConnection}}" />
  </appSettings>
