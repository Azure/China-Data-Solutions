{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "chinanorth"
        },
        "virtualMachineName": {
            "value": "{{Name}}"
        },
        "virtualMachineSize": {
            "value": "Standard_DS2"
        },
        "adminUsername": {
            "value": "azure"
        },
        "storageAccountName": {
            "value": "{{Name}}stor"
        },
        "virtualNetworkName": {
            "value": "{{Name}}-vnet"
        },
        "networkInterfaceName": {
            "value": "{{Name}}Netinterface"
        },
        "networkSecurityGroupName": {
            "value": "{{Name}}-nsg"
        },
        "adminPassword": {
            "value": "{{Password}}"
        },
        "storageAccountType": {
            "value": "Premium_LRS"
        },
        "diagnosticsStorageAccountName": {
            "value": "{{Name}}dstor"
        },
        "diagnosticsStorageAccountType": {
            "value": "Standard_LRS"
        },
        "diagnosticsStorageAccountId": {
            "value": "Microsoft.Storage/storageAccounts/{{Name}}dstor"
        },
        "addressPrefix": {
            "value": "10.2.0.0/24"
