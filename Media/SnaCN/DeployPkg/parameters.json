        "subnetName": {
            "value": "default"
        },
        "subnetPrefix": {
            "value": "10.2.0.0/24"
        },
        "publicIpAddressName": {
            "value": "{{Name}}-ip"
        },
        "publicIpAddressType": {
            "value": "Static"
        },
		"servers_sndemoserver_name": {
            "value": "{{Name}}dbserver"
        },
        "serverfarms_SNADemo_name": {
            "value": "{{Name}}appsf"
        },
        "sites_SNADemo_name": {
            "value": "{{Name}}sites"
        },
		"databases_SNADemo_Id": {
            "value": "{{DBID}}"
        },
        "databases_master_name": {
            "value": "{{Name}}dbserver/master"
        },
        "databases_SNADemo_name": {
            "value": "{{Name}}dbserver/SNADemo"
        },
        "firewallRules_AllowAllWindowsAzureIps_name": {
            "value": "{{Name}}dbserver/AllowAllWindowsAzureIps"
        }
    }
}